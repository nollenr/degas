<% provide(:title, 'User Index') %>

<div class="hero-unit">
  <h2>Users</h2>
</div>

<div class="user-index">
  <table class="table table-bordered table-condensed">
    <thead>
      <th>Username</th>
      <th>E-Mail</th>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td> <%= user.username %> </td>
          <td> <%= user.email %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
