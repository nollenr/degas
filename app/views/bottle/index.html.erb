<div class="hero-unit">
  <h2>Bottles</h2>
</div>

<%= link_to "Showing All (toggle)", bottle_index_path(show_avail_next: true), id: 'toggle' %>

<p>Before:  <%= @before_changes %></p>
<p>Has Key: <%= @params_has_key %></p>
<p>After:   <%= @after_changes  %></p>

<table class="table table-hover" id='bottle'>
  <tr>
    <th>Bottle Number</th>
    <th>Grape</th>
  </tr>

  <%= render @bottles %>

</table>
